{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nycpayroll-ws"
		},
		"datalake_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake_ls'"
		},
		"sql_db_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sql_db_ls'"
		},
		"synapse_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'synapse_ls'"
		},
		"datalake_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectudacitysa.dfs.core.windows.net/"
		},
		"synapse_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pipelinesynapseproject-ondemand.sql.azuresynapse.net"
		},
		"synapse_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "udacity"
		},
		"synapse_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nycpayroll-ws_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake_ls_accountKey": {
						"value": "[parameters('datalake_ls_accountKey')]"
					},
					"sql_db_ls_connectionString": {
						"value": "[parameters('sql_db_ls_connectionString')]"
					},
					"synapse_ls_password": {
						"value": "[parameters('synapse_ls_password')]"
					},
					"datalake_ls_properties_typeProperties_url": {
						"value": "[parameters('datalake_ls_properties_typeProperties_url')]"
					},
					"synapse_ls_properties_typeProperties_server": {
						"value": "[parameters('synapse_ls_properties_typeProperties_server')]"
					},
					"synapse_ls_properties_typeProperties_database": {
						"value": "[parameters('synapse_ls_properties_typeProperties_database')]"
					},
					"synapse_ls_properties_typeProperties_userName": {
						"value": "[parameters('synapse_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "nycpayroll-ws_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'nycpayroll-ws_ArmTemplate_0')]"
			]
		}
	]
}