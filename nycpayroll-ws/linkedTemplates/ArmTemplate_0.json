			"defaultValue": "nycpayroll-ws"
		},
		"datalake_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake_ls'"
		},
		"sql_db_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sql_db_ls'"
		},
		"synapse_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'synapse_ls'"
		},
		"datalake_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectudacitysa.dfs.core.windows.net/"
		},
		"synapse_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pipelinesynapseproject-ondemand.sql.azuresynapse.net"
		},
		"synapse_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "udacity"
		},
		"synapse_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		}
	},
